{
  "expo": {
    "name": "typescript-instaClone",
    "slug": "typescript-instaClone",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/instagram_logo.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/instagram_logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "plugins":[
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you share them with friends."
        }
      ]
    ],
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}